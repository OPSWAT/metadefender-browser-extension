{
  "name": "opswat-file-security-chrome",
  "private": true,
  "version": "3.9.0",
  "description": "Scan files and downloads with OPSWAT File Security for Chrome",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:OPSWAT/metadefender-browser-extension.git"
  },
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "node ./node_modules/eslint/bin/eslint.js . -f visualstudio",
    "lint-fix": "node ./node_modules/eslint/bin/eslint.js . --fix -f visualstudio",
    "test": "karma start karma.conf.js --browsers ChromeHeadless",
    "test-ci": "karma start karma.conf.js --browsers CustomChromiumHeadless",
    "test-docker": "docker run -v `pwd`:/opt/mo -it --rm opswat/metadefender-browser-extension:latest sh -c 'npm run test-ci'"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "dependencies": {
    "angular": "1.7.9",
    "angular-route": "1.7.9",
    "angular-sanitize": "1.7.9",
    "bootstrap": "3.4.1",
    "browser-cookies": "1.2.0",
    "moment": "2.24.0",
    "spark-md5": "3.0.0",
    "uniqid": "5.0.3",
    "webextension-polyfill": "0.4.0",
    "xhr": "2.5.0"
  },
  "devDependencies": {
    "angular-mocks": "1.7.9",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-istanbul": "5.1.4",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "chromereload": "0.0.2",
    "debounce": "1.2.0",
    "del": "4.1.1",
    "eslint": "6.0.0",
    "eslint-plugin-jasmine": "2.10.1",
    "gulp": "3.9.1",
    "gulp-bump": "3.1.3",
    "gulp-cache": "1.1.2",
    "gulp-clean-css": "4.2.0",
    "gulp-filter": "6.0.0",
    "gulp-git": "2.9.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "6.0.0",
    "gulp-json-transform": "0.4.6",
    "gulp-less": "4.0.1",
    "gulp-livereload": "4.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-sass": "4.0.2",
    "gulp-sequence": "1.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-tag-version": "1.3.1",
    "gulp-util": "3.0.8",
    "gulp-zip": "5.0.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "3.4.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.5",
    "require-dir": "1.2.0",
    "sinon": "^7.3.2",
    "sinon-chrome": "3.0.1",
    "standard": "12.0.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "vinyl-named": "1.1.0",
    "webpack": "3.12.0",
    "webpack-stream": "3.2.0",
    "yargs": "13.2.4"
  }
}
