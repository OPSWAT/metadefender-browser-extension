{
  "name": "opswat-file-security-chrome",
  "private": true,
  "version": "3.9.3",
  "description": "Scan files and downloads with OPSWAT File Security for Chrome",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:OPSWAT/metadefender-browser-extension.git"
  },
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "node ./node_modules/eslint/bin/eslint.js . -f visualstudio",
    "lint-fix": "node ./node_modules/eslint/bin/eslint.js . --fix -f visualstudio",
    "test": "karma start karma.conf.js --browsers ChromeHeadless",
    "test-ci": "karma start karma.conf.js --browsers CustomChromiumHeadless",
    "test-docker": "docker run -v `pwd`:/opt/mo -it --rm opswat/metadefender-browser-extension:latest sh -c 'npm run test-ci'"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "dependencies": {
    "@popperjs/core": "2.11.2",
    "angular": "1.8.2",
    "angular-route": "1.8.2",
    "angular-sanitize": "1.8.2",
    "bootstrap": "5.1.3",
    "browser-cookies": "1.2.0",
    "mime-types": "2.1.34",
    "moment": "2.29.2",
    "sass": "1.49.0",
    "spark-md5": "3.0.2",
    "uniqid": "5.4.0",
    "webextension-polyfill": "0.8.0",
    "xhr": "2.6.0"
  },
  "devDependencies": {
    "angular-mocks": "1.8.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.2.3",
    "babel-plugin-istanbul": "6.1.1",
    "babel-preset-env": "1.7.0",
    "chai": "4.3.6",
    "chromereload": "0.0.2",
    "debounce": "1.2.1",
    "del": "6.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-jasmine": "4.1.3",
    "gulp": "4.0.2",
    "gulp-bump": "3.2.0",
    "gulp-cache": "1.1.3",
    "gulp-clean-css": "4.3.0",
    "gulp-filter": "7.0.0",
    "gulp-git": "2.10.1",
    "gulp-if": "3.0.0",
    "gulp-imagemin": "7.1.0",
    "gulp-json-transform": "0.4.7",
    "gulp-less": "5.0.0",
    "gulp-livereload": "4.0.2",
    "gulp-plumber": "1.2.1",
    "gulp-sass": "5.1.0",
    "gulp-sequence": "1.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-tag-version": "1.3.1",
    "gulp-util": "3.0.8",
    "gulp-zip": "5.1.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "4.0.2",
    "karma": "6.3.12",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-sourcemap-loader": "0.3.8",
    "karma-spec-reporter": "0.0.33",
    "karma-webpack": "5.0.0",
    "require-dir": "1.2.0",
    "sinon": "12.0.1",
    "sinon-chrome": "3.0.1",
    "standard": "16.0.4",
    "uglifyjs-webpack-plugin": "2.2.0",
    "vinyl-named": "1.1.0",
    "webpack": "5.67.0",
    "webpack-stream": "7.0.0",
    "yargs": "17.3.1"
  }
}
